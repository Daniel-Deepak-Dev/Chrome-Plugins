{
  "manifest_version": 2,
  "name": "Easy GHO",
  "version": "1.0",
  "description": "This extension is used to add default GHO template to cases we havent put any comment on in our shifts",
  " ": [
    {
      "matches": ["https://orgcs.my.salesforce.com/*"],
      "js": ["jsforce.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "https://orgcs.lightning.force.com/",
    "https://orgcs.my.salesforce.com/services/data/v37.0",
    "https://orgcs.my.salesforce.com/services/data/v42.0",
    "alarms",
    "clipboardWrite",
    "cookies",
    "https://orgcs.my.salesforce.com/*",
    "https://login.Salesforce.com/*",
    "storage"
  ]
}
