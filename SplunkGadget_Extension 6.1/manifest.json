{
  "manifest_version": 2,

  "name": "Splunk Related Plugin 6.1",
  "description": "This extension will Redirect to splunk page and few automated dashboards",
  "version": "6.1",
   
  "background" :{
  "scripts" : ["background.js"]
  },
  "browser_action": {
   "default_icon": "icon1.png"   
  },
  "permissions": [
      "tabs",
	  "https://prom-gadget.eng.sfdc.net/diagnostics/*",
	  "https://prom-gadgetui2-1-xrd.eng.sfdc.net:8443/diagnostics/*",
	  "https://internal-gadgetui-xrd.eng.sfdc.net/diagnostics/*",	  
	  "https://gadget-prod.herokuapp.com/diagnostics/*", 
    "https://gadget.prom.sfdc.sh/diagnostics/*"
    ]
  
}