{
	"manifest_version": 2,
	"name": "Special Handing Instructions",
	"version": "1.0",
	"description": "This extension is used to disply Account Special Handing Instructions",
	"content_scripts": [
	  {
		"matches": ["https://org62.my.salesforce.com/*"],
		"js": ["jsforce.js"]
	  }
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup" : "popup.html"
	},
	"permissions":[
		"tabs",
		"activeTab",
		"https://prom-gadget.eng.sfdc.net/diagnostics/*",
		"https://prom-gadgetui2-1-xrd.eng.sfdc.net:8443/diagnostics/*",
		"https://org62.lightning.force.com/",
		"https://org62.my.salesforce.com/services/data/v37.0",
		"https://org62.my.salesforce.com/services/data/v42.0",
		"https://mcsorg.my.salesforce.com/",		
		"https://mcsorg.lightning.force.com/",
		"alarms",
		"clipboardWrite",
        "cookies",
        "https://org62.my.salesforce.com/*",
        "https://login.Salesforce.com/*",
		"https://media.geeksforgeeks.org/*",
		"https://docs.google.com/spreadsheets/d/1sbyMINQHPsJctjAtMW0lCfLrcpMqoGMOJj6AN-sNQrc/pubhtml",
		"https://cdnjs.cloudflare.com/ajax/libs/tabletop.js/1.5.1/tabletop.min.js",
		"storage"
	]
  
}